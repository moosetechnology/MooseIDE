Class {
	#name : #RSAttributedTextExamples,
	#superclass : #RSAttributedText,
	#category : #'MooseIDE-AttributedText'
}

{ #category : #examples }
RSAttributedTextExamples class >> example01 [
	"TagEmphasis"
	<example>
	| string |
	string := 'context function foo base 123 context
function 123
  123function123
  123   context    context
fun
  context'.

	^self new
		text: string ;
		tagAll: 'context'  in: string with: [RSTagEmphasisAttribute new color: Color red ; on: RSMouseClick do: [ :event | UIManager default inform: 'A red Context' ]] ;
		tagAll: 'function' in: string with: [RSTagEmphasisAttribute new color: Color green  ; on: RSMouseClick do: [ :event | UIManager default inform: 'A green Function' ]] ;
		open
	
]

{ #category : #examples }
RSAttributedTextExamples class >> example02 [
	"LinkEmphasis"
	<example>
	| string |
	string := 'context function foo base 123 context
function 123
  123function123
  123   context    context
fun
  context'.

	^self new
		text: string ;
		tagAll: 'function' in: string with: [ RSLinkEmphasisAttribute new on: RSMouseClick do: [ :event | UIManager default inform: 'This is a function' ] ] ;
		open
	
]

{ #category : #examples }
RSAttributedTextExamples class >> example03 [	
	"mixing TagEmphasis and LinkEmphasis on the same substring"
	<example>
	| string |
	string := 'context function foo base 123 context
function 123
  123function123
  123   context    context
fun
  context'.

	self new
		text: string ;
		tagAll: 'context'  in: string with: [RSTagEmphasisAttribute new color: Color red  ; on: RSMouseClick do: [ :event | UIManager default inform: 'A red Context' ]] ;
		tagAll: 'function' in: string with: [RSTagEmphasisAttribute new color: Color green  ; on: RSMouseClick do: [ :event | UIManager default inform: 'A green Function' ]] ;
		tagAll: 'function' in: string with: [RSLinkEmphasisAttribute new  on: RSMouseClick do: [ :event | UIManager default inform: 'A simple Function' ]] ;
		open
]

{ #category : #examples }
RSAttributedTextExamples class >> example04 [
	"several lines"
	<example>
	| string |
	string := 'cinco
uno cuatro
     dos
tres
foooo'.

	self new
		text: string ;
		addAttribute: (RSTagEmphasisAttribute new color: Color blue  ; on: RSMouseClick do: [ :event | UIManager default inform: 'A blue emphasis' ]) from: 7 to: 25 ;
		open
	
]

{ #category : #examples }
RSAttributedTextExamples class >> example05 [
	"big text"
	<example>
	| string |
	string := String fromByteArray: self sourceCodeExample base64Decoded.

	self new
		text: string ;
		tagAll: 'context'  in: string with: [RSTagEmphasisAttribute new color: Color yellow  ; on: RSMouseClick do: [ :event | UIManager default inform: 'A yellow Context' ]] ;
		tagAll: 'function' in: string with: [RSLinkEmphasisAttribute new on: RSMouseClick do: [ :event | UIManager default inform: 'A simple Function' ]] ;
		open	
]

{ #category : #examples }
RSAttributedTextExamples class >> sourceCodeExample [
	^ ''
]

{ #category : #examples }
RSAttributedTextExamples >> tagAll: word in: aString with: anAttributeBlock [
	(aString allRangesOfSubstring: word) do: [ :range |
		self 
			addAttribute: anAttributeBlock value
			from: range first 
			to: range last.
	]
]
